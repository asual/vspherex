{
  "extends": "tslint:latest",
  "rules": {
    "align": [
      false
    ],
    "class-name": false,
    "file-header": [
      true,
      "Copyright \\(c\\) \\d{4} Rostislav Hristov"
    ],
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      80
    ],
    "member-access": [
      false
    ],
    "no-console": [
      false
    ],
    "no-namespace": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "prefer-const": [
      false
    ],
    "trailing-comma": [
      false
    ]
  }
}
